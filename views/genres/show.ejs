<%- include('../partials/html-head') %>
<link rel='stylesheet' href='/stylesheets/genres/show.css'>
<%- include('../partials/nav') %> 

<main>
  <h1><%= title %></h1>
  <div id="genre-specific-div">
    <ul>
        <% songs.forEach(song => { %>
          <li class="songs-in-list">
            🎵 
            <%= song.title %> 
            - <%= song.artist %>
            <a href="/songs/<%= song._id %>"><button class="details-btn">Details</button></a>
          </li>
        <% }) %>
      </ul>
      </div>
<%- include('../partials/footer') %>